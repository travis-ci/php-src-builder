#!/bin/bash
set -o xtrace
set -o errexit
source $(dirname $0)/compile-extensions-common

travis_time_start

if [[ ! $VERSION =~ ^7 && ! $VERSION =~ ^8 && ! $VERSION =~ ^master$ ]]; then
	pecl download memcache-beta
	tar zxvf memcache*.tgz && pushd memcache*/
	make clean || true
	phpize
	./configure && make && make install
	popd
fi

travis_time_finish
