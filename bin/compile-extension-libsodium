#!/bin/bash
set -o xtrace
set -o errexit
source $(dirname $0)/compile-extensions-common

travis_time_start

if [[ $VERSION =~ ^7 ]]; then
    git clone -b stable https://github.com/jedisct1/libsodium.git
    pushd libsodium
    ./configure
    make check
    make install
    popd
fi

travis_time_end

true
