#!/bin/bash
set -o xtrace
set -o errexit
source $(dirname $0)/compile-extensions-common

source ~/.asdf/asdf.sh
asdf global php ${VERSION}

travis_time_start

if [[ ! $VERSION =~ ^7 && ! $VERSION =~ ^8 && ! $VERSION =~ ^master$ ]]; then
	apcu=apcu-4.0.11
else
	apcu=apcu
fi

pecl channel-update pecl.php.net

echo | pecl_install $apcu

travis_time_finish

true
